{
  "name": "client",
  "version": "0.1.0",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:client": "next dev -p 3000",
    "dev:hcm": "hcm '**/*.module.css' -w",
    "dev:aspida": "aspida --watch",
    "dev:path": "pathpida --ignorePath ../.gitignore -s -w",
    "build": "npm run generate && next build",
    "start": "sh -c \"next start -p $(($PORT + 1))\"",
    "generate": "run-p generate:*",
    "generate:aspida": "aspida",
    "generate:path": "pathpida --ignorePath ../.gitignore -s",
    "hcm": "hcm '**/*.module.css'",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:updateSnapshot": "vitest -u",
    "typecheck": "npm run hcm && tsc --noEmit"
  },
  "dependencies": {
    "@aspida/axios": "^1.14.0",
    "@aspida/swr": "^1.14.0",
    "@aws-amplify/ui-react": "^6.1.13",
    "@mapbox/mapbox-gl-language": "^1.0.1",
    "aspida": "^1.14.0",
    "aws-amplify": "^6.4.0",
    "axios": "^1.7.2",
    "html2canvas": "^1.4.1",
    "jotai": "^2.9.0",
    "mapbox-gl": "^3.5.2",
    "next": "^14.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-use-websocket": "^4.8.1",
    "swr": "^2.2.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/mapbox-gl": "^3.1.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "happy-css-modules": "^3.1.1",
    "notios": "^0.5.3",
    "pathpida": "^0.22.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
